{
  "expo": {
    "name": "GigBud",
    "slug": "GigBud",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "scheme": "gigbud",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
        "image": "./assets/images/splash.png",
        "backgroundColor": "#18305c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.churtdev.GigBud",
      "buildNumber": "1.1.0",
      "icon": "./assets/images/ios_icon.png"
     },
    "android": {
      "package": "com.churtdev.GigBud",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "versionCode": 3
    },

  }
}
