{
  "expo": {
    "name": "GigBud",
    "slug": "GigBud",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "scheme": "gigbud",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "backgroundColor": "#07337A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.churtdev.GigBud"
     },
    "android": {
      "package": "com.churtdev.GigBud",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
     }
  }
}
